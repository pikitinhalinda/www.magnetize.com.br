<script>
ModelProduto = function(produto){
	var self = this;
	
	var props = Object.getOwnPropertyNames(produto);
	for(var i = 0; i < props.length; i++){
		var propriedade = props[i];
		self[propriedade] = ko.observable(produto[propriedade]);
	}
	
	self.quantidade.subscribe(function(val){
		$.ajax({
			type: 'post',
			url: "\/orcamento\/alterar-produto-detalhes",
			data: {
				id: self.id(),
				produto: self.produto_id(),
				quantidade: self.quantidade(),
				tela: 'orcamento',
			},
			processData: true,
			contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
			headers: {
				'X-CSRF-Token': "b90530fe4a432a65677535574da6f78275fef9e449541cf0b84f40a6f67d9076a8f04cd2e2691c2cb04cbb99de148d511655e8512a5b54cb44187e9328cec403",
			},
		});
	});
	
	self.variacao_selecionada = ko.observable();
	
	if (self.variacao_id()) {
		for (let i = 0; i < self.variacoes().length; i++){
			if (self.variacoes()[i].id == self.variacao_id()){
				self.variacao_selecionada(self.variacoes()[i]);
				break;
			}
		}
	}
	
	self.variacao_selecionada.subscribe(function(newVal){
		
		if (typeof self.variacao_selecionada() != 'object'){
			var id_variacao = '';
		}
		else {
			var id_variacao = self.variacao_selecionada().id;
		}
		
		$.ajax({
			url: "\/orcamento\/alterar-variacao",
			type: 'post',
			data: {
				id: self.id(),
				variacao: id_variacao,
			},
			headers: {
				'X-CSRF-Token': "b90530fe4a432a65677535574da6f78275fef9e449541cf0b84f40a6f67d9076a8f04cd2e2691c2cb04cbb99de148d511655e8512a5b54cb44187e9328cec403",
			},
		});
	});
	
	if (self.variacoes().length == 1 && typeof self.variacao_selecionada() != 'object'){
		self.variacao_selecionada(self.variacoes()[0]);
	}
	
	for (let i = 0; i < self.variacoes().length; i++){
		for (let j = 0; j < self.variacoes()[i].propriedades.length; j++){
			// usar 'let' para 'propriedade' ficar acessível corretamente dentro do observer-subscribe
			let propriedade = self.variacoes()[i].propriedades[j];
			propriedade.propriedade_preenchida = ko.observable();
			
			if (typeof self.variacoes_propriedades()[propriedade.id] != 'undefined'){
				propriedade.propriedade_preenchida(self.variacoes_propriedades()[propriedade.id]);
			}
			
			propriedade.propriedade_preenchida.subscribe(function(newVal){
					
				$.ajax({
					url: "\/orcamento\/alterar-variacao-propriedade",
					type: 'post',
					data: {
						id: self.id(),
						variacao: self.variacao_selecionada().id,
						propriedade: propriedade.id,
						valor: newVal,
					},
					headers: {
						'X-CSRF-Token': "b90530fe4a432a65677535574da6f78275fef9e449541cf0b84f40a6f67d9076a8f04cd2e2691c2cb04cbb99de148d511655e8512a5b54cb44187e9328cec403",
					},
				});
			});
		}
	}
};

ModelOrcamento = function(){
	var self = this;
	
	self.produtos = ko.observableArray([]);
	
		
	self.remover_item = function(produto){
		self.produtos.remove(produto);
		
		$.ajax({
			url: "\/orcamento\/remover-produto-orcamento",
			type: 'post',
			data: {
				id: produto.id(),
				produto: produto.produto_id(),
			},
			headers: {
				'X-CSRF-Token': "b90530fe4a432a65677535574da6f78275fef9e449541cf0b84f40a6f67d9076a8f04cd2e2691c2cb04cbb99de148d511655e8512a5b54cb44187e9328cec403",
			},
		});
	};
};

var model = new ModelOrcamento();
ko.applyBindings(model, document.getElementById('orcamento-section'));

/***** função de validação *******/

validaOrcamentos = function(f){
	
		
	return true;
}


</script>

